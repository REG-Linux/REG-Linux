+/dts-v1/;
+
+#include <dt-bindings/input/gpio-keys.h>
+#include <dt-bindings/input/input.h>
+#include <dt-bindings/gpio/gpio.h>
+#include <dt-bindings/pinctrl/qcom,pmic-gpio.h>
+#include <dt-bindings/regulator/qcom,rpmh-regulator.h>
+#include <dt-bindings/interrupt-controller/irq.h>
+#include <dt-bindings/input/linux-event-codes.h>
+#include <dt-bindings/sound/qcom,q6afe.h>
+#include <dt-bindings/sound/qcom,q6asm.h>
+#include <dt-bindings/leds/common.h>
+#include "sdm845.dtsi"
+#include "sdm845-wcd9340.dtsi"
+#include "pm8998.dtsi"
+#include "pmi8998.dtsi"
+#include "pm8005.dtsi"
+
+/ {
+	model = "AYN Odin";
+	compatible = "ayn,odin", "qcom,sdm845", "qcom,mtp";
+	qcom,msm-id = <321 0x20001>;
+	qcom,board-id = <8 0>;
+	chassis-type = "tablet";
+
+	aliases {
+		serial0 = &uart9;
+		hsuart0 = &uart6;
+		wifi0 = &wifi;
+	};
+
+	chosen {
+		stdout-path = "serial0:115200n8";
+	};
+
+/* Fixed crystal oscillator dedicated to MCP2517FD */
+	clk40M: can-clock {
+		compatible = "fixed-clock";
+		#clock-cells = <0>;
+		clock-frequency = <40000000>;
+	};
+
+	gpio-keys {
+		compatible = "gpio-keys";
+		pinctrl-names = "default";
+		pinctrl-0 = <&btn_pins>;
+		label = "Odin Gamepad Buttons";
+		
+		/* Button A / East */
+		btn_east {
+			label = "Button A";
+			linux,code = <BTN_EAST>;
+			gpios = <&tlmm 96 GPIO_ACTIVE_LOW>;
+			debounce-interval = <10>; 
+		};
+
+		/* Button B / South */
+		btn_south {
+			label = "Button B";
+			linux,code = <BTN_SOUTH>;
+			gpios = <&tlmm 95 GPIO_ACTIVE_LOW>;
+			debounce-interval = <10>;
+		};
+
+		/* Button X / West */
+		btn_west {
+			label = "Button X";
+			linux,code = <BTN_WEST>;
+			gpios = <&tlmm 41 GPIO_ACTIVE_LOW>;
+			debounce-interval = <10>;
+		};
+
+		/* Button Y / North */
+		btn_north {
+			label = "Button Y";
+			linux,code = <BTN_NORTH>;
+			gpios = <&tlmm 92 GPIO_ACTIVE_LOW>;
+			debounce-interval = <10>;
+		};
+
+		/* Left Shoulder / L1 */
+		btn_tl {
+			label = "L1";
+			linux,code = <BTN_TL>;
+			gpios = <&tlmm 26 GPIO_ACTIVE_LOW>;
+			debounce-interval = <10>;
+		};
+
+		/* Right Shoulder / R1 */
+		btn_tr {
+			label = "R1";
+			linux,code = <BTN_TR>;
+			gpios = <&tlmm 91 GPIO_ACTIVE_LOW>;
+			debounce-interval = <10>;
+		};
+
+		/* Select */
+		btn_select {
+			label = "Select";
+			linux,code = <BTN_SELECT>;
+			gpios = <&tlmm 0 GPIO_ACTIVE_LOW>;
+			debounce-interval = <10>;
+		};
+
+		/* Start */
+		btn_start {
+			label = "Start";
+			linux,code = <BTN_START>;
+			gpios = <&tlmm 3 GPIO_ACTIVE_LOW>;
+			debounce-interval = <10>;
+		};
+
+		/* Left Stick Click / L3 */
+		btn_thumbl {
+			label = "L3";
+			linux,code = <BTN_THUMBL>;
+			gpios = <&tlmm 40 GPIO_ACTIVE_LOW>;
+			debounce-interval = <10>;
+		};
+
+		/* Right Stick Click / R3 */
+		btn_thumbr {
+			label = "R3";
+			linux,code = <BTN_THUMBR>;
+			gpios = <&tlmm 22 GPIO_ACTIVE_LOW>;
+			debounce-interval = <10>;
+		};
+
+		/* D-Pad Up */
+		btn_dpad_up {
+			label = "D-Pad Up";
+			linux,code = <BTN_DPAD_UP>;
+			gpios = <&tlmm 124 GPIO_ACTIVE_LOW>;
+			debounce-interval = <10>;
+		};
+
+		/* D-Pad Down */
+		btn_dpad_down {
+			label = "D-Pad Down";
+			linux,code = <BTN_DPAD_DOWN>;
+			gpios = <&tlmm 128 GPIO_ACTIVE_LOW>;
+			debounce-interval = <10>;
+		};
+
+		/* D-Pad Left */
+		btn_dpad_left {
+			label = "D-Pad Left";
+			linux,code = <BTN_DPAD_LEFT>;
+			gpios = <&tlmm 52 GPIO_ACTIVE_LOW>;
+			debounce-interval = <10>;
+		};
+
+		/* D-Pad Right */
+		btn_dpad_right {
+			label = "D-Pad Right";
+			linux,code = <BTN_DPAD_RIGHT>;
+			gpios = <&tlmm 122 GPIO_ACTIVE_LOW>;
+			debounce-interval = <10>;
+		};
+
+		/* Mode Button */
+		btn_mode {
+			label = "Mode";
+			linux,code = <BTN_MODE>;
+			gpios = <&tlmm 49 GPIO_ACTIVE_LOW>;
+			debounce-interval = <10>;
+		};
+
+		vol-up {
+			label = "Volume Up";
+			linux,code = <KEY_VOLUMEUP>;
+			gpios = <&pm8998_gpios 6 GPIO_ACTIVE_LOW>;
+			pinctrl-names = "default";
+			pinctrl-0 = <&volume_up_gpio>;
+		};
+	};
+
+	adc-joystick {
+		compatible = "adc-joystick";
+		pinctrl-names = "default";
+		pinctrl-0 = <&btn_en_active>;
+		
+		poll-interval = <10>; 
+
+		io-channels = <&pm8998_adc ADC5_GPIO2>,
+		              <&pm8998_adc ADC5_GPIO3>,
+		              <&pm8998_adc ADC5_GPIO5>,
+		              <&pm8998_adc ADC5_GPIO6_DIV3>,
+		              <&pm8998_adc ADC5_GPIO1>,
+		              <&pm8998_adc ADC5_GPIO4>;
+
+		#address-cells = <1>;
+		#size-cells = <0>;
+
+		/* Left Stick X */
+		axis@0 {
+			reg = <0>;
+			linux,code = <ABS_X>;
+			abs-range = <0 12000>;
+			abs-fuzz = <30>;
+			abs-flat = <30>;
+		};
+
+		/* Left Stick Y */
+		axis@1 {
+			reg = <1>;
+			linux,code = <ABS_Y>;
+			abs-range = <0 12000>;
+			abs-fuzz = <30>;
+			abs-flat = <30>;
+		};
+
+		/* Right Stick X */
+		axis@2 {
+			reg = <2>;
+			linux,code = <ABS_RX>;
+			abs-range = <0 12000>;
+			abs-fuzz = <30>;
+			abs-flat = <30>;
+		};
+
+		/* Right Stick Y */
+		axis@3 {
+			reg = <3>;
+			linux,code = <ABS_RY>;
+			abs-range = <0 12000>;
+			abs-fuzz = <30>;
+			abs-flat = <30>;
+		};
+
+		/* Left Trigger (L2) */
+		axis@4 {
+			reg = <4>;
+			linux,code = <ABS_Z>;
+			abs-range = <0 12000>;
+			abs-fuzz = <30>;
+			abs-flat = <30>;
+		};
+
+		/* Right Trigger (R2) */
+		axis@5 {
+			reg = <5>;
+			linux,code = <ABS_RZ>;
+			abs-range = <0 12000>;
+			abs-fuzz = <30>;
+			abs-flat = <30>;
+		};
+	};
+
+	fan_en_reg: fan-en-regulator {
+		compatible = "regulator-fixed";
+		regulator-name = "fan_en";
+
+		regulator-min-microvolt = <5000000>;
+		regulator-max-microvolt = <5000000>;
+
+		gpio = <&tlmm 44 GPIO_ACTIVE_HIGH>;
+		enable-active-high;
+	};
+
+        pwm_fan: pwm-fan {
+                compatible = "pwm-fan";
+                cooling-levels = <0 64 128 192 255>;
+                #cooling-cells = <2>;
+                fan-supply = <&fan_en_reg>;
+                pinctrl-0 = <&pwm_fan_ctrl>;
+                pinctrl-names = "default";
+                pwms = <&pmi8998_lpg 1 50000>;
+        };
+
+	thermal-zones {
+    cluster0-thermal {
+        trips {
+            cluster0_fan0: cluster0-fan0 {
+                temperature = <50000>;
+                hysteresis = <2000>;
+                type = "active";
+            };
+            cluster0_fan1: cluster0-fan1 {
+                temperature = <60000>;
+                hysteresis = <2000>;
+                type = "active";
+            };
+            cluster0_fan2: cluster0-fan2 {
+                temperature = <70000>;
+                hysteresis = <2000>;
+                type = "active";
+            };
+        };
+        cooling-maps {
+            map1 { trip = <&cluster0_fan0>; cooling-device = <&pwm_fan 0 1>; };
+            map2 { trip = <&cluster0_fan1>; cooling-device = <&pwm_fan 1 3>; };
+            map3 { trip = <&cluster0_fan2>; cooling-device = <&pwm_fan 3 4>; };
+        };
+    };
+    cluster1-thermal {
+        trips {
+            cluster1_fan0: cluster1-fan0 { temperature = <50000>; hysteresis = <2000>; type = "active"; };
+            cluster1_fan1: cluster1-fan1 { temperature = <60000>; hysteresis = <2000>; type = "active"; };
+            cluster1_fan2: cluster1-fan2 { temperature = <70000>; hysteresis = <2000>; type = "active"; };
+        };
+        cooling-maps {
+            map1 { trip = <&cluster1_fan0>; cooling-device = <&pwm_fan 0 1>; };
+            map2 { trip = <&cluster1_fan1>; cooling-device = <&pwm_fan 1 3>; };
+            map3 { trip = <&cluster1_fan2>; cooling-device = <&pwm_fan 3 4>; };
+        };
+    };
+    gpu-top-thermal {
+        trips {
+            gpu_top_fan0: gpu-top-fan0 { temperature = <50000>; hysteresis = <2000>; type = "active"; };
+            gpu_top_fan1: gpu-top-fan1 { temperature = <60000>; hysteresis = <2000>; type = "active"; };
+            gpu_top_fan2: gpu-top-fan2 { temperature = <70000>; hysteresis = <2000>; type = "active"; };
+        };
+        cooling-maps {
+            map1 { trip = <&gpu_top_fan0>; cooling-device = <&pwm_fan 0 1>; };
+            map2 { trip = <&gpu_top_fan1>; cooling-device = <&pwm_fan 1 3>; };
+            map3 { trip = <&gpu_top_fan2>; cooling-device = <&pwm_fan 3 4>; };
+        };
+    };
+    gpu-bottom-thermal {
+        trips {
+            gpu_bottom_fan0: gpu-bottom-fan0 { temperature = <50000>; hysteresis = <2000>; type = "active"; };
+            gpu_bottom_fan1: gpu-bottom-fan1 { temperature = <60000>; hysteresis = <2000>; type = "active"; };
+            gpu_bottom_fan2: gpu-bottom-fan2 { temperature = <70000>; hysteresis = <2000>; type = "active"; };
+        };
+        cooling-maps {
+            map1 { trip = <&gpu_bottom_fan0>; cooling-device = <&pwm_fan 0 1>; };
+            map2 { trip = <&gpu_bottom_fan1>; cooling-device = <&pwm_fan 1 3>; };
+            map3 { trip = <&gpu_bottom_fan2>; cooling-device = <&pwm_fan 3 4>; };
+        };
+    };
+};
+
+	leds {
+		compatible = "gpio-leds";
+
+		pinctrl-names = "default";
+		pinctrl-0 = <&gpio_leds_default>;
+
+		led-0 {
+			color = <LED_COLOR_ID_BLUE>;
+			gpios = <&tlmm 7 GPIO_ACTIVE_HIGH>;
+			label = "right_joystick";
+		};
+
+		led-1 {
+			color = <LED_COLOR_ID_BLUE>;
+			gpios = <&tlmm 25 GPIO_ACTIVE_HIGH>;
+			label = "left_joystick";
+		};
+
+		led-2 {
+			color = <LED_COLOR_ID_BLUE>;
+			gpios = <&tlmm 87 GPIO_ACTIVE_HIGH>;
+			label = "right_side";
+		};
+
+		led-3 {
+			color = <LED_COLOR_ID_BLUE>;
+			gpios = <&tlmm 129 GPIO_ACTIVE_HIGH>;
+			label = "left_side";
+		};
+	};
+
+	battery: battery {
+		compatible = "simple-battery";
+
+		/* Basic capacity info */
+		charge-full-design-microamp-hours = <6600000>;
+		voltage-min-design-microvolt = <3600000>;
+		voltage-max-design-microvolt = <4400000>;
+	};
+
+	connector {
+		compatible = "hdmi-connector";
+
+		label = "hdmi";
+		type = "d";
+
+		hdmi-pwr-supply = <&hdmi_en_reg>;
+		hpd-gpios = <&tlmm 121 GPIO_ACTIVE_HIGH>;
+
+		pinctrl-names = "default";
+		pinctrl-0 = <&hdmi_hpd>;
+
+		port {
+			hdmi_con: endpoint {
+				remote-endpoint = <&lt8912b_out>;
+			};
+		};
+	};
+
+	hdmi_en_reg: hdmi-en-regulator {
+		compatible = "regulator-fixed";
+		regulator-name = "hdmi_en";
+
+		gpio = <&tlmm 13 GPIO_ACTIVE_HIGH>;
+		enable-active-high;
+	};
+
+	vph_pwr: vph-pwr-regulator {
+		compatible = "regulator-fixed";
+		regulator-name = "vph_pwr";
+		regulator-min-microvolt = <3700000>;
+		regulator-max-microvolt = <3700000>;
+	};
+
+	vreg_s4a_1p8: pm8998-smps4 {
+		compatible = "regulator-fixed";
+		regulator-name = "vreg_s4a_1p8";
+
+		regulator-min-microvolt = <1800000>;
+		regulator-max-microvolt = <1800000>;
+
+		regulator-always-on;
+		regulator-boot-on;
+		vin-supply = <&vph_pwr>;
+	};
+};
+
+&adsp_pas {
+	status = "okay";
+	firmware-name = "qcom/sdm845/adsp.mbn";
+};
+
