+    ((sizeof(ptr_t) == 8) ? (\
+     (((ptr_t)vcmd_get_register_value((reg_base),(reg_mirror), name)) |  \
+     (((ptr_t)vcmd_get_register_value((reg_base), (reg_mirror),name##_MSB)) << 32))\
+    ) : ((ptr_t)vcmd_get_register_value((reg_base),(reg_mirror), (name))))
+
