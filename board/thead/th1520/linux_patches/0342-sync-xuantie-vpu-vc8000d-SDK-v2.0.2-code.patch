+static int CheckDecIrq(hantrodec_t *dev, int id) {
+  unsigned long flags;
+  int rdy = 0;
+
+  const u32 irq_mask = (1 << id);
+
+  spin_lock_irqsave(&owner_lock, flags);
+
+  if(dec_irq & irq_mask) {
+    /* reset the wait condition(s) */
+    dec_irq &= ~irq_mask;
+    rdy = 1;
+  }
+
+  spin_unlock_irqrestore(&owner_lock, flags);
+
+  return rdy;
+}
