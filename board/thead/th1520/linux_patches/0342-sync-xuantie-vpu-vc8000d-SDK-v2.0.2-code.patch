+  ret = copy_from_user(dec_regs[id] + HANTRO_PP_ORG_FIRST_REG,
+                       core->regs + HANTRO_PP_ORG_FIRST_REG,
+                       HANTRO_PP_ORG_REGS*4);
+  if(sizeof(addr_t) == 8) {
+    /* copy extended dec regs to kernal space*/
+    ret = copy_from_user(dec_regs[id] + HANTRO_PP_EXT_FIRST_REG,
