+  allocate_cmdbuf(&new_cmdbuf_addr, &new_status_cmdbuf_addr);
+#else
+  if (wait_event_interruptible(vcmd_cmdbuf_memory_wait, allocate_cmdbuf(&new_cmdbuf_addr, &new_status_cmdbuf_addr)))
+    return NULL;
