+long ReservePostProcessor(hantrodec_t *dev, int filp) {
+  unsigned long flags;
+
+  long core = 0;
+
+  /* single core PP only */
+  if (down_interruptible(&pp_core_sem))
+    return -ERESTARTSYS;
+
+  spin_lock_irqsave(&owner_lock, flags);
+
+  pp_owner[core] = filp;
+
+  spin_unlock_irqrestore(&owner_lock, flags);
+
+  return core;
