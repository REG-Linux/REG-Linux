+      wake_up_all(dev->wait_abort_queue);
+      wake_up_all(&mc_wait_queue);
+      handled++;
+      return IRQ_HANDLED;
+    }
+    if(vcmd_get_register_mirror_value(dev->reg_mirror,HWIF_VCMD_IRQ_BUSERR))
