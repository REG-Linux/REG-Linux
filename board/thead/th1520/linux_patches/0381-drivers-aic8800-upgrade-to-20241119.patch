+#define CMD_SET_VAR_STR     "SET_VAR"
+
+enum custmsg_cmd_tag
+{
+	CUST_CMD_GET_VERSION = 0,
+	CUST_CMD_GET_SSID,
+	CUST_CMD_SET_SSID,
+	CUST_CMD_GET_PASS,
+	CUST_CMD_SET_PASS,
+	CUST_CMD_GET_VAR,
+	CUST_CMD_SET_VAR,
+	CUST_CMD_MAX
