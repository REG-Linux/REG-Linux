+  BOOST_CHECK_EQUAL(set.size(), nb_values);
+
+  for (std::size_t i = 0; i < nb_values; i++) {
+    const std::size_t insert_val = (i % 2 == 0) ? i : nb_values + i;
+    std::tie(it, inserted) = set.insert(utils::get_key<key_t>(insert_val));
